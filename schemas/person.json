{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "additionalProperties": true,
  "properties": {
    "primary": {
      "job": {
        "properties": {
          "$ref": "includes/experience.json"
        }
      },
      "location": {
        "properties": {
          "$ref": "includes/location.json"
        }
      },
      "linkedin": {
        "type": [
          "string",
          "null"
        ]
      },
      "work_emails": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "personal_emails": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "other_emails": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "name": {
        "properties": {
          "first_name": {
            "type": "string"
          },
          "middle_name": {
            "type": "string"
          },
          "last_name": {
            "type": "string"
          },
          "clean": {
            "type": "string"
          }
        },
        "type": "object"
      }
    },
    "birth_date": {
      "type": [
        "string",
        "null"
      ],
      "format": "full-date"
    },
    "birth_date_fuzzy": {
      "type": [
        "string",
        "null"
      ],
      "format": "date-fullyear"
    },
    "gender": {
      "type": "string",
      "enum": [
        "male",
        "female",
        null
      ]
    },
    "education": {
      "items": {
        "$ref": "includes/education.json"
      },
      "type": "array"
    },
    "emails": {
      "items": {
        "additionalProperties": true,
        "properties": {
          "address": {
            "examples": [
              "sean.thorne@peopledatalabs.com"
            ],
            "type": "string"
          },
          "domain": {
            "examples": [
              "peopledatalabs.com"
            ],
            "type": "string"
          },
          "local": {
            "examples": [
              "sean.thorne"
            ],
            "type": "string"
          },
          "type": {
            "enum": [
              "professional",
              "personal",
              "current_professional",
              null
            ],
            "type": [
              "string",
              "null"
            ]
          }
        },
        "required": [
          "address",
          "domain",
          "local"
        ],
        "type": "object"
      },
      "type": "array"
    },
    "experience": {
      "items": {
        "$ref": "includes/experience.json"
      },
      "type": "array"
    },
    "industries": {
      "items": {
        "$ref": "includes/industry.json"
      },
      "type": "array"
    },
    "interests": {
      "items": {
        "additionalProperties": true,
        "properties": {
          "name": {
            "examples": [
              "digital media",
              "going on hikes",
              "baseball"
            ],
            "type": "string"
          }
        },
        "required": [
          "name"
        ],
        "type": "object"
      },
      "type": "array"
    },
    "locations": {
      "items": {
        "$ref": "includes/location.json"
      },
      "type": "array"
    },
    "names": {
      "items": {
        "$ref": "includes/person_name.json"
      },
      "type": "array"
    },
    "phone_numbers": {
      "items": {
        "additionalProperties": true,
        "properties": {
          "E164": {
            "examples": [
              "+14152756100"
            ],
            "type": "string"
          },
          "country_code": {
            "examples": [
              "1"
            ],
            "type": "string"
          },
          "national_number": {
            "examples": [
              "4152756100"
            ],
            "type": "string"
          },
          "number": {
            "examples": [
              "+14152756100"
            ],
            "type": "string"
          },
          "type": {
            "enum": [
              "home",
              "personal",
              "professional",
              "work",
              null
            ],
            "type": "string"
          }
        },
        "type": "object"
      },
      "required": [
        "E164",
        "number"
      ],
      "type": "array"
    },
    "profiles": {
      "items": {
        "$ref": "includes/profile.json"
      },
      "type": "array"
    },
    "skills": {
      "items": {
        "additionalProperties": true,
        "properties": {
          "name": {
            "examples": [
              "nonprofits",
              "python",
              "data science"
            ],
            "type": "string"
          }
        },
        "required": [
          "name"
        ],
        "type": "object"
      },
      "type": "array"
    }
  },
  "type": "object"
}